{
  "$schema": "./node_modules/@openapitools/openapi-generator-cli/config.schema.json",
  "spaces": 2,
  "generator-cli": {
    "version": "6.0.1",
    "generators": {
      "go": {
        "generatorName": "go",
        "input-spec": "openapi.yml",
        "output": "#{cwd}/client",
        "additionalProperties": {
          "packageName": "client"
        }
      }
    }
  }
}
